# zipPack custom entry coverage.
tempFile pack-zip!

[
    {
        path: `zip/base/alpha.txt`,
        archivePath: "packed/alpha.txt",
        mode: 420
    }
    {
        path: `zip/extra/extra.txt`,
        archivePath: "extras/extra.txt",
        mode: 420
    }
] pack-items!

@pack-items @pack-zip zipPack

"zipPack entries" wl
@pack-zip zipList (
    dup "name" get?
    swap
    dup "mode" get?
    swap drop
    2tuple
    str
) map sort str wl

"zipPack alpha" wl
@pack-zip "packed/alpha.txt" zipRead ? utf8Str wl

"zipPack extra" wl
@pack-zip "extras/extra.txt" zipRead ? utf8Str wl
